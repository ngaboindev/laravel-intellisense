{
  "name": "laravel-intellisense",
  "displayName": "laravel intellisense",
  "description": "Laravel intellisense for Visual Studio Code",
  "version": "0.1.0",
  "publisher": "mohamedbenhida",
  "icon": "img/laravel.png",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "keywords": [
    "php",
    "intellisense",
    "laravel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/simoebenhida/laravel-intellisense"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "generate_ide_helper",
        "title": "Generate Ide Helper"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Laravel Intellisense",
      "properties": {
        "LaravelIntellisense.model": {
          "type": "string",
          "scope": "resource",
          "default": "App\\Models",
          "description": "The model namespace."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "php-parser": "^3.0.1"
  }
}
